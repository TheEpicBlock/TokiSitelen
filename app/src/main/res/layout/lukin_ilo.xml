<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:theme="@style/lukinKonSuli"
    android:id="@+id/keyboard"
    android:layout_alignParentBottom="true"

    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <GridLayout
        android:id="@+id/kamaNenaMute"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:columnCount="10">

    </GridLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50sp">

        <ImageButton
            style="@style/nenaWawa"
            android:id="@+id/kamaAla"
            android:layout_width="0dp"
            android:layout_weight="2"
            android:layout_height="match_parent"
            android:src="@drawable/sitelen_kon_ala">

        </ImageButton>
        <Button
            style="@style/nenaWawa"
            android:id="@+id/kamaInsaNimi"
            android:layout_width="0dp"
            android:layout_weight="2"
            android:layout_height="match_parent"
            android:text="Â·">

        </Button>
        <ImageButton
            style="@style/nena"
            android:id="@+id/kamaAnte"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:src="@drawable/sitelen_kon_ante">

        </ImageButton>
    </LinearLayout>
</LinearLayout>
